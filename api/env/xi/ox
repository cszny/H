-- // Variables 
local Library = loadstring((game:HttpGet("https://hikarishit.vercel.app/library")))()
local Notify = loadstring(game:HttpGet("https://hikarishit.vercel.app/notify"))()
local VirtualUser = game:GetService('VirtualUser')
local HttpService = game:GetService("HttpService")
local UserInputService = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")

local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local PlaceId = game.PlaceId
local Remote_GetFruits = ReplicatedStorage:FindFirstChild("Remotes").CommF_:InvokeServer("GetFruits");

local CombatFramework = require(game:GetService("Players").LocalPlayer.PlayerScripts:WaitForChild("CombatFramework"))
local CombatFrameworkR = getupvalues(CombatFramework)[2]
local RigController = require(game:GetService("Players")["LocalPlayer"].PlayerScripts.CombatFramework.RigController)
local RigControllerR = getupvalues(RigController)[2]
local realbhit = require(game.ReplicatedStorage.CombatFramework.RigLib)
local cooldownfastattack = tick()

local YaY = require(game:GetService("Players").LocalPlayer.PlayerScripts.CombatFramework)
local CameraShakerR = require(game.ReplicatedStorage.Util.CameraShaker)

-- // Global Environment
getgenv().Check_Save_Setting = "CheckSaveSetting"
getgenv().Team = "Marines"
getgenv().Rejoin = false
getgenv().GetBoss = false
getgenv().First_World = false
getgenv().New_World = false
getgenv().Third_World = false
getgenv().onJoined = Players.LocalPlayer.Neutral == false

-- // Repeats
repeat wait() until game:IsLoaded()
repeat wait() until Players
repeat wait() until Players.LocalPlayer
repeat wait() until Players.LocalPlayer.PlayerGui
repeat wait() until ReplicatedStorage.Effect.Container

if not game:IsLoaded() then
	Notify.New("[Auxiliary]: Game Loading...")
end

-- // Starter Functions
do
if (getgenv().Team == "Pirates" or getgenv().Team == "Marines") and not getgenv().onJoined then
	repeat wait()
		pcall(function()
			getgenv().onJoined = Players.localPlayer.Neutral == false
			if getgenv().Team == "Pirates" then
				for i,v in pairs({"MouseButton1Click", "MouseButton1Down", "Activated"}) do
					for i,v in pairs(getconnections(Players.LocalPlayer.PlayerGui.Main.ChooseTeam.Container.Pirates.Frame.ViewportFrame.TextButton[v])) do
						v.Function()
					end
				end
			elseif getgenv().Team == "Marines" then
				for i,v in pairs({"MouseButton1Click", "MouseButton1Down", "Activated"}) do
					for i,v in pairs(getconnections(Players.LocalPlayer.PlayerGui.Main.ChooseTeam.Container.Marines.Frame.ViewportFrame.TextButton[v])) do
						v.Function()
					end
				end
			else
				for i,v in pairs({"MouseButton1Click", "MouseButton1Down", "Activated"}) do
					for i,v in pairs(getconnections(Players.LocalPlayer.PlayerGui.Main.ChooseTeam.Container.Pirates.Frame.ViewportFrame.TextButton[v])) do
						v.Function()
					end
				end
			end
		end)
	until getgenv().onJoined == true and Players.LocalPlayer.Team ~= nil and game:IsLoaded() 
end
if PlaceId == 2753915549 then
	getgenv().First_World = true
elseif PlaceId == 4442272183 then
	getgenv().New_World = true
elseif PlaceId == 7449423635 then
	getgenv().Third_World = true
end
Players.LocalPlayer.Idled:connect(function()
	VirtualUser:CaptureController()
	VirtualUser:ClickButton2(Vector2.new())
end)
spawn(function()
	while wait(3) do
    VirtualUser:CaptureController()
	end
end)
end

