			TelePBoss(CFrameQ)
			
		elseif Lv == 15 or Lv <= 29 or SelectMonster == "Gorilla" then -- Gorilla
			Ms = "Gorilla"
			NameQuest = "JungleQuest"
			QuestLv = 2
