function GetBossName()
	for i,v in pairs(game.ReplicatedStorage:GetChildren()) do
		if not getgenv().GetBoss then
			-- World 1
			if v.Name == "The Gorilla King" and Players.localPlayer.Data.Level.Value >= 20  then
				getgenv().Settings_Table.Selectb = "The Gorilla King"
			elseif v.Name == "Bobby" and Players.localPlayer.Data.Level.Value >= 55  then
				getgenv().Settings_Table.Selectb = "Bobby" 
			elseif v.Name == "Yeti" and Players.localPlayer.Data.Level.Value >= 105  then
				getgenv().Settings_Table.Selectb = "Yeti"
			elseif v.Name == "Mob Leader"  and Players.localPlayer.Data.Level.Value >= 120 then
				getgenv().Settings_Table.Selectb = "Mob Leader"
			elseif v.Name == "Vice Admiral"  and Players.localPlayer.Data.Level.Value >= 130 then
				getgenv().Settings_Table.Selectb = "Vice Admiral"
			elseif v.Name == "Warden" and Players.localPlayer.Data.Level.Value >= 175 then
				getgenv().Settings_Table.Selectb = "Warden"
			elseif v.Name == "Saber Expert" and game.Workspace.Map.Jungle.Final.Part.Transparency == 1 then
				getgenv().Settings_Table.Selectb = "Saber Expert"
			elseif v.Name == "Chief Warden"  and Players.localPlayer.Data.Level.Value >= 200 then
				getgenv().Settings_Table.Selectb = "Chief Warden"
			elseif v.Name == "Swan"  and Players.localPlayer.Data.Level.Value >= 250 then
				getgenv().Settings_Table.Selectb = "Swan"
			elseif v.Name == "Magma Admiral" and Players.localPlayer.Data.Level.Value >= 350  then
				getgenv().Settings_Table.Selectb = "Magma Admiral"
			elseif v.Name == "Fishman Lord" and Players.localPlayer.Data.Level.Value >= 425  then
				getgenv().Settings_Table.Selectb = "Fishman Lord"
			elseif v.Name == "Wysper" and Players.localPlayer.Data.Level.Value >= 500 then
				getgenv().Settings_Table.Selectb = "Wysper"
			elseif v.Name == "Thunder God"  and Players.localPlayer.Data.Level.Value >= 575 then
				getgenv().Settings_Table.Selectb = "Thunder God"
			elseif v.Name == "Cyborg" and Players.localPlayer.Data.Level.Value >= 675 then
				getgenv().Settings_Table.Selectb = "Cyborg"
				-- World2
			elseif v.Name == "Diamond" and Players.localPlayer.Data.Level.Value >= 750 then
				getgenv().Settings_Table.Selectb = "Diamond"
			elseif v.Name == "Jeremy" and Players.localPlayer.Data.Level.Value >= 850 then
				getgenv().Settings_Table.Selectb = "Jeremy"
			elseif v.Name == "Fajita" and Players.localPlayer.Data.Level.Value >= 925  then
				getgenv().Settings_Table.Selectb = "Fajita"
			elseif v.Name == "Don Swan" and Players.localPlayer.Data.Level.Value >= 1000 and ReplicatedStorage.Remotes.CommF_:InvokeServer("TalkTrevor","1") == 0 then
				getgenv().Settings_Table.Selectb = "Don Swan" 
			elseif v.Name == "Smoke Admiral" and Players.localPlayer.Data.Level.Value >= 1150 then
				getgenv().Settings_Table.Selectb = "Smoke Admiral"
			elseif v.Name == "Cursed Captain" and Players.localPlayer.Data.Level.Value >= 1325 then
				getgenv().Settings_Table.Selectb = "Cursed Captain"
			elseif v.Name == "Awakened Ice Admiral" and Players.localPlayer.Data.Level.Value >= 1400  then
				getgenv().Settings_Table.Selectb = "Awakened Ice Admiral"
			elseif v.Name == "Tide Keeper" and Players.localPlayer.Data.Level.Value >= 1475  then
				getgenv().Settings_Table.Selectb = "Tide Keeper"
				-- World3
			elseif v.Name == "Stone" and Players.localPlayer.Data.Level.Value >= 1550 then
				getgenv().Settings_Table.Selectb = "Stone"
			elseif v.Name == "Island Empress" and Players.localPlayer.Data.Level.Value >= 1675 then
				getgenv().Settings_Table.Selectb = "Island Empress"
			elseif v.Name == "Kilo Admiral" and Players.localPlayer.Data.Level.Value >= 1750 then
				getgenv().Settings_Table.Selectb = "Kilo Admiral"
			elseif v.Name == "Captain Elephant" and Players.localPlayer.Data.Level.Value >= 1875 then
				getgenv().Settings_Table.Selectb = "Captain Elephant"
			elseif v.Name == "Beautiful Pirate" and Players.localPlayer.Data.Level.Value >= 1950 then
				getgenv().Settings_Table.Selectb = "Beautiful Pirate"
			elseif v.Name == "Cake Queen" and Players.localPlayer.Data.Level.Value >= 2175 then
				getgenv().Settings_Table.Selectb = "Cake Queen"
			end 
		end 
	end
end
function CheckLevel()
	local Lv = Players.LocalPlayer.Data.Level.Value
	if getgenv().First_World then
		if Lv == 1 or Lv <= 9 or SelectMonster == "Bandit" then -- Bandit
			Ms = "Bandit"
			NameQuest = "BanditQuest1"
			QuestLv = 1
			NameMon = "Bandit"
			CFrameQ = CFrame.new(1045.962646484375, 27.00250816345215, 1560.8203125)
			CFrameMon = CFrame.new(1038.5533447266, 41.296249389648, 1576.5098876953)
		elseif Lv == 10 or Lv <= 14 or SelectMonster == "Monkey" then -- Monkey
			Ms = "Monkey"
			NameQuest = "JungleQuest"
			QuestLv = 1
			NameMon = "Monkey"
			CFrameQ = CFrame.new(-1448.51806640625, 67.85301208496094, 11.46579647064209)
			CFrameMon = CFrame.new(-1448.1446533203, 50.851993560791, 63.60718536377)
		elseif Lv == 15 or Lv <= 29 or SelectMonster == "Gorilla" then -- Gorilla
			Ms = "Gorilla"
			NameQuest = "JungleQuest"
			QuestLv = 2
			NameMon = "Gorilla"
			CFrameQ = CFrame.new(-1129.8836669921875, 40.46354675292969, -525.4237060546875)
			CFrameMon = CFrame.new(-1142.6488037109, 40.462348937988, -515.39227294922)
		elseif Lv == 30 or Lv <= 39 or SelectMonster == "Pirate" then -- Pirate
			Ms = "Pirate"
