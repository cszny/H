-- // Main Functions
do
function TelePBoss(p)
	pcall(function()
		if (p.Position-Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude >= 2000 and not Auto_Raid and Players.LocalPlayer.Character.Humanoid.Health > 0 then
			if NameQuest == "FishmanQuest" then
				_G.Stop_Tween = true
				TP(Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
				wait()
				ReplicatedStorage.Remotes.CommF_:InvokeServer("requestEntrance",Vector3.new(61163.8515625, 11.6796875, 1819.7841796875))
				_G.Stop_Tween = nil
			elseif Ms == "God's Guard [Lv. 450]"  then
				_G.Stop_Tween = true
				TP(Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
				wait()
				ReplicatedStorage.Remotes.CommF_:InvokeServer("requestEntrance",Vector3.new(-4607.82275, 872.54248, -1667.55688))
				_G.Stop_Tween = nil
			elseif NameQuest == "SkyExp1Quest" then
				_G.Stop_Tween = true
				TP(Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
				wait()
				ReplicatedStorage.Remotes.CommF_:InvokeServer("requestEntrance",Vector3.new(-7894.6176757813, 5547.1416015625, -380.29119873047))
				_G.Stop_Tween = nil
			elseif NameQuest == "ShipQuest1" then
				_G.Stop_Tween = true
				TP(Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
				wait()
				ReplicatedStorage.Remotes.CommF_:InvokeServer("requestEntrance",Vector3.new(923.21252441406, 126.9760055542, 32852.83203125))
				_G.Stop_Tween = nil
			elseif NameQuest == "ShipQuest2" then
				_G.Stop_Tween = true
				TP(Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
				wait()
				ReplicatedStorage.Remotes.CommF_:InvokeServer("requestEntrance",Vector3.new(923.21252441406, 126.9760055542, 32852.83203125))
				_G.Stop_Tween = nil
			elseif NameQuest == "FrostQuest" then
				_G.Stop_Tween = true
				TP(Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
				wait()
				ReplicatedStorage.Remotes.CommF_:InvokeServer("requestEntrance",Vector3.new(-6508.5581054688, 89.034996032715, -132.83953857422))
				_G.Stop_Tween = nil
			else
				Mix_Farm = true
				_G.Stop_Tween = true
				repeat wait()
					Players.LocalPlayer.Character.HumanoidRootPart.CFrame = p
					wait(.05)
					Players.LocalPlayer.Character.Head:Destroy()
					Players.LocalPlayer.Character.HumanoidRootPart.CFrame = p
				until (p.Position-Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude < 1500 and Players.LocalPlayer.Character.Humanoid.Health > 0
				wait()
				_G.Stop_Tween = nil
				Mix_Farm = nil
			end
		end
	end)
end
