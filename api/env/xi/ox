-- Fruits / Components
Fruits:AddLabel({
  Text = "Fruits",
  Emoji = "🍎"
})
Fruits:AddDropdown({
  Name = "Select Fruit",
  Default = "",
  Options = Table_DevilFruitSniper,
  Callback = function(v)
    _G.Select_Devil_Fruit = v
    getgenv().Settings_Table.Select_Devil_Fruit = v
  end
})
Fruits:AddToggle({
  Name = "Auto Buy Selected Fruit",
  Default = false,
  Callback = function(v)
    _G.Auto_Buy_Devil_Fruit = v
    getgenv().Settings_Table.Auto_Buy_Devil_Fruit = v
  end
})
Fruits:AddButton({
  Name = "Open Fruit Shop",
  Callback = function()
    game:GetService("Players").LocalPlayer.PlayerGui.Main.FruitShop.Visible = true
  end
})
Fruits:AddToggle({
  Name = "Auto Random Fruit",
  Default = false,
  Callback = function(v)
    _G.Auto_Random_Fruit = v
    getgenv().Settings_Table.Auto_Random_Fruit = v
  end
})
Fruits:AddToggle({
  Name = "Auto Bring Fruit",
  Default = false,
  Callback = function(v)
    BringFruit = v
    getgenv().Settings_Table.BringFruit = v
    spawn(function()
		while wait(.2) do
			pcall(function()
				if BringFruit or BringFruit_P then
				   	for i,v6 in pairs(game:GetService("Workspace"):GetChildren()) do
				   		if v6:IsA ("Tool") and (v6.Handle.Position-game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude < 13000 then
				   			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v6.Handle.CFrame
				   			v6.Handle.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
				   		end
				   	end
				end				
			end)
		end
	end)
  end
})
Fruits:AddToggle({
  Name = "Auto Store Fruit",
  Default = false,
  Callback = function(v)
    getgenv().Settings_Table.Auto_Store_Fruit = v
  end
})