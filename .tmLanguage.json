{
  "name": "Hikari",
  "scopeName": "source.Hikari",
  "fileTypes": ["hkr", "Hikari"],
  "patterns": [
    {
      "name": "keyword.Hikari",
      "match": "\\b(hk|hkr|hkh)\\b"
    },
    {
      "name": "string.Hikari",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "escape.Hikari",
          "match": "\\\\."
        }
      ]
    }
  ]
}
